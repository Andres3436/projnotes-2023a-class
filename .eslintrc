{
  "parser": "@babel/eslint-parser",
  "extends":["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules":{
    "no-console":"off",
    "prettier/prettier": ["error"],
    "comma-dangle": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  }, 
  "globals": {
    "__rootdir": "readonly"
},
"settings": {
  "import/resolver" : { "babel-module" : {}}
}
}